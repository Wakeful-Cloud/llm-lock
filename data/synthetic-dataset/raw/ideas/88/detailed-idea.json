{
  "name": "NameForge",
  "description": "NameForge is an AI\u2011powered product naming assistant that blends brand tone, SEO keyword insight, trademark clearance, and domain availability checks to deliver ready\u2011to\u2011launch, memorable product names for startups, marketers, and product teams.",
  "tools": [
    {
      "name": "seo_keyword_research",
      "description": "Performs SEO keyword research for a given product concept and returns a list of high\u2011volume, low\u2011competition keywords that can inspire product names.",
      "parameters": {
        "type": "object",
        "properties": {
          "concept": {
            "type": "string",
            "description": "A short description of the product or market segment to research."
          },
          "max_keywords": {
            "type": "integer",
            "description": "Maximum number of keywords to return. Defaults to 10 if omitted."
          }
        },
        "required": [
          "concept"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "generate_product_names",
      "description": "Generates a list of candidate product names based on brand tone, a set of seed keywords, and optional naming constraints such as length or word count.",
      "parameters": {
        "type": "object",
        "properties": {
          "tone": {
            "type": "string",
            "description": "Desired brand voice (e.g., \"fresh, playful\", \"innovative, premium\")."
          },
          "keywords": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Seed SEO keywords that the names should incorporate or echo."
          },
          "max_length": {
            "type": "integer",
            "description": "Maximum character count for each generated name."
          },
          "max_words": {
            "type": "integer",
            "description": "Maximum number of words per name."
          },
          "count": {
            "type": "integer",
            "description": "How many name candidates to generate. Defaults to 5."
          }
        },
        "required": [
          "tone",
          "keywords"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "trademark_search",
      "description": "Looks up a name in a public trademark database (USPTO, EUIPO, etc.) and returns registration status, class, and any conflicting marks.",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The exact product name to check for existing trademarks."
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "domain_availability_check",
      "description": "Queries a domain\u2011registration API to see whether the .com version of a name is currently available for registration.",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The name (without extension) to test for .com availability."
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    }
  ],
  "examples": [
    {
      "description": "User asks for three fresh, playful names for an eco\u2011friendly water bottle; the assistant runs SEO research, generates names, and validates trademark and .com availability.",
      "messages": [
        {
          "role": "system",
          "content": "You are NameForge, an AI assistant that creates product names using brand tone, SEO keywords, trademark checks, and domain availability verification."
        },
        {
          "role": "user",
          "content": "I need a name for a new eco\u2011friendly reusable water bottle. Our brand tone is fresh and playful. Please give me three options and tell me if the trademarks and .com domains are available."
        },
        {
          "role": "assistant",
          "content": "Let me first discover relevant SEO keywords for your product.",
          "tool_calls": [
            {
              "id": "seo_keyword_research_a1b2c3",
              "name": "seo_keyword_research",
              "arguments": {
                "concept": "eco\u2011friendly reusable water bottle",
                "max_keywords": 5
              }
            }
          ]
        },
        {
          "role": "tool",
          "tool_call_id": "seo_keyword_research_a1b2c3",
          "content": "{\"keywords\": [\"sustainable hydration\", \"green bottle\", \"eco drinkware\", \"reusable water\", \"environmental sip\"]}"
        },
        {
          "role": "assistant",
          "content": "Great, using those keywords I\u2019ll generate three name ideas that fit a fresh, playful tone and are no longer than 12 characters.",
          "tool_calls": [
            {
              "id": "generate_product_names_d4e5f6",
              "name": "generate_product_names",
              "arguments": {
                "tone": "fresh, playful",
                "keywords": [
                  "sustainable hydration",
                  "green bottle",
                  "eco drinkware",
                  "reusable water",
                  "environmental sip"
                ],
                "max_length": 12,
                "count": 3
              }
            }
          ]
        },
        {
          "role": "tool",
          "tool_call_id": "generate_product_names_d4e5f6",
          "content": "{\"names\": [\"EcoSip\", \"GreenFizz\", \"FreshFlow\"]}"
        },
        {
          "role": "assistant",
          "content": "Now I\u2019ll check each name for trademark conflicts and .com domain availability.",
          "tool_calls": [
            {
              "id": "trademark_search_g7h8i9",
              "name": "trademark_search",
              "arguments": {
                "name": "EcoSip"
              }
            },
            {
              "id": "trademark_search_j0k1l2",
              "name": "trademark_search",
              "arguments": {
                "name": "GreenFizz"
              }
            },
            {
              "id": "trademark_search_m3n4o5",
              "name": "trademark_search",
              "arguments": {
                "name": "FreshFlow"
              }
            },
            {
              "id": "domain_availability_check_p6q7r8",
              "name": "domain_availability_check",
              "arguments": {
                "name": "EcoSip"
