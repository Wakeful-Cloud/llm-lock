        {
          "id": "generate_care_plan_1a2b3c",
          "name": "generate_care_plan",
          "arguments": {
            "patient_id": "P12345",
            "ehr_summary": "{\"diagnoses\": [\"I10\"], \"medications\": [\"Lisinopril\"]}",
