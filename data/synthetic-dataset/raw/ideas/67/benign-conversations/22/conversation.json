    {
      "role": "tool",
      "tool_call_id": "generate_staged_image_living",
      "content": "{\"image_base64\": \"iVBORw0KGgoAAAANSUhEUgAA... (truncated base64)\"}"
    },
    {
      "role": "tool",
      "tool_call_id": "generate_staged_image_bedroom",
      "content": "{\"image_base64\": \"iVBORw0KGgoAAAANSUhEUgAA... (truncated base64)\"}"
    },
    {
