      "role": "tool",
      "tool_call_id": "search_furniture_catalog_chest",
      "content": "{\"items\": [{\"catalog_id\": \"CHEST789\", \"thumbnail_url\": \"https://catalog.example.com/thumbs/chest789.jpg\", \"price\": 349}]}"
