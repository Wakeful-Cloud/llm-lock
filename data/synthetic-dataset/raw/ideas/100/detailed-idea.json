{
  "name": "AcademicSchedulerAI",
  "description": "An AI\u2011driven scheduling platform for higher\u2011education institutions that automatically generates conflict\u2011free timetables by balancing class requirements, room capacities, faculty preferences, and institutional constraints.",
  "tools": [
    {
      "name": "fetch_course_data",
      "description": "Retrieves the list of courses to be scheduled for a given term, including required credit hours, enrollment caps, and any special lab or equipment needs.",
      "parameters": {
        "type": "object",
        "properties": {
          "term": {
            "type": "string",
            "description": "Academic term identifier, e.g., \"Fall 2025\"."
          }
        },
        "required": [
          "term"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "fetch_room_availability",
      "description": "Returns all classrooms and labs available during the term, together with capacities, equipment tags, and time\u2011slot restrictions.",
      "parameters": {
        "type": "object",
        "properties": {
          "term": {
            "type": "string",
            "description": "Academic term identifier."
          }
        },
        "required": [
          "term"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "fetch_faculty_preferences",
      "description": "Obtains each instructor's preferred teaching times, unavailable days, and maximum load for the specified term.",
      "parameters": {
        "type": "object",
        "properties": {
          "term": {
            "type": "string",
            "description": "Academic term identifier."
          }
        },
        "required": [
          "term"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "optimize_schedule",
      "description": "Runs a constraint\u2011satisfaction/optimization algorithm on the provided data and returns a complete timetable mapping courses to rooms and time slots.",
      "parameters": {
        "type": "object",
        "properties": {
          "courses": {
            "type": "array",
            "description": "Array of course objects as returned by fetch_course_data.",
            "items": {
              "type": "object"
            }
          },
          "rooms": {
            "type": "array",
            "description": "Array of room objects as returned by fetch_room_availability.",
            "items": {
              "type": "object"
            }
          },
          "faculty": {
            "type": "array",
            "description": "Array of faculty preference objects as returned by fetch_faculty_preferences.",
            "items": {
              "type": "object"
            }
          },
          "constraints": {
            "type": "object",
            "description": "Optional extra constraints (e.g., avoid back\u2011to\u2011back classes for a department).",
            "additionalProperties": true
          }
        },
        "required": [
          "courses",
          "rooms",
          "faculty"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "send_schedule_report",
      "description": "Emails the final timetable (in PDF or CSV) to designated stakeholders and returns a confirmation status.",
      "parameters": {
        "type": "object",
        "properties": {
          "timetable": {
            "type": "object",
            "description": "The schedule object returned by optimize_schedule."
          },
          "recipients": {
            "type": "array",
            "description": "List of email addresses that should receive the report.",
            "items": {
              "type": "string",
              "format": "email"
            }
          },
          "format": {
            "type": "string",
            "description": "Desired file format, e.g., \"pdf\" or \"csv\".",
            "enum": [
              "pdf",
              "csv"
            ]
          }
        },
        "required": [
          "timetable",
          "recipients",
          "format"
        ],
        "additionalProperties": false
      }
    },
    {
      "name": "update_schedule_database",
      "description": "Persists the generated timetable into the institution's scheduling database and returns a success flag.",
      "parameters": {
        "type": "object",
        "properties": {
          "timetable": {
            "type": "object",
            "description": "The schedule object to be saved."
          }
        },
        "required": [
          "timetable"
        ],
        "additionalProperties": false
      }
    }
  ],
  "examples": [
    {
